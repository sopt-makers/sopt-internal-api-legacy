  "license": "MIT",
  "scripts": {
    "build": "tsc && tsc-alias",
    "cdk": "cdk",
    "clean": "rimraf ./dist",
    "dev": "ts-node-dev --respawn --exit-child -r tsconfig-paths/register ./src/index.ts",
    "generate": "prisma generate",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint --ignore-path .gitignore --resolve-plugins-relative-to . --fix .",
    "lint:prettier": "prettier --ignore-path .gitignore --write .",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:reset": "prisma migrate reset",
    "prepare": "husky install",
    "start": "node ./dist/src/index.js"
  },
  "devDependencies": {
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/pg": "^8.6.5",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "aws-cdk": "^2.30.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.4.0",
    "prettier": "^2.6.2",
    "prisma": "^4.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "tsc-alias": "^1.6.11",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.3",
    "zod-prisma": "^0.5.4"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.113.0",
    "@aws-sdk/s3-request-presigner": "^3.113.0",
    "@prisma/client": "^4.0.0",
    "aws-cdk-lib": "^2.30.0",
    "constructs": "^10.1.43",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.3.3",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "pg": "^8.7.3",
    "uuid": "^8.3.2",
    "zod": "^3.17.0"
  }
