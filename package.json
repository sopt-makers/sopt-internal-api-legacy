    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "prisma": "^3.14.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "ts-to-zod": "^1.10.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.3",
    "zod-prisma": "^0.5.4"
  },
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.3.3",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "pg": "^8.7.3",
    "zod": "^3.17.0"
